<%= form_for(:session, :html => {role: "form"}, url: login_path) do |f| %>
      <%= f.label :Username %>

      <%= f.text_field :name, placeholder: "Username", autofocus: true %>

      <%= f.label :password %>

      <%= f.password_field :password, autocomplete: "off" %>


      <%= f.submit "Log in" %>

<% end %>